{
  "title": "Shared Learning Notes",
  "author": "star.you",
  "description": "swpe Learning Notes",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "plugins": [
    "theme-comscore",
    "-sharing",
    "-lunr",
    "-search",
    "search-pro-fixed",
    "splitter",
    "editlink",
    "back-to-top-button",
    "code",
    "prism",
    "-highlight",
    "chapter-fold",
    "expandable-chapters",
    "advanced-emoji",
    "page-toc-button",
    "pageview-count",
    "git-author",
    "hide-published-with",
    "page-footer-ex"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ]
    },
    "editlink": {
      "base": "http://10.18.6.89/helin.liu/docs/tree/master/",
      "label": "Edit this page"
    },
    "git-author": {
      "position": "bottom",
      "modifyTpl": "Last modified by {user} at {timeStamp}",
      "createTpl": "Author: {user}, Created on {timeStamp}",
      "timeStampFormat": "YYYY-MM-DD HH:mm:ss"
    },
    "page-footer-ex": {
      "copyright": "Copyright &copy 2020 Helin Liu. All Rights Reserved.",
      "markdown": false,
      "update_label": "<i> </i>",
      "update_format": " "
    }
  }
}
